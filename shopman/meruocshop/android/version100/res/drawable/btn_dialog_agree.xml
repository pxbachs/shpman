<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_selected="false" android:state_pressed="false">
		<layer-list>
		    <item>
	    		<bitmap android:gravity="center" android:src="@drawable/agree"/>
		    </item>			
	    </layer-list>
	</item>		
	<item android:state_pressed="true">
		<layer-list>
		    <item>
	    		<bitmap android:gravity="center" android:src="@drawable/agree_pressed"/>
		    </item>			
	    </layer-list>
	</item>	
</selector>
